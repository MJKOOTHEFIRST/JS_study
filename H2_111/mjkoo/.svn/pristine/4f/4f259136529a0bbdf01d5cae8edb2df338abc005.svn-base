  float: right;
  width: calc(100% - 50px);
  height: 12px;
}
.widget.soft-senser-monitoring .result .outline {
  margin: 5px 0;
  height: 7px;
  width: 100%;
  border: 1px solid var(--normal-color);
  background: var(--normal-color);
}
.widget.soft-senser-monitoring .line {
  position: relative;
  float: left;
  background: #EBEBEB;
  background: var(--normal-color);
  height: 7px;
  width: 10%;
  width: 4.166%;
  color: var(--normal-color);
}
.widget.soft-senser-monitoring .tooltip-box {
  position: absolute;
  background: inherit !important;
  top: 13px;
}
.widget.soft-senser-monitoring .tooltip-box:hover {
  z-index: 999;
}
.widget.soft-senser-monitoring .tooltip-box::before {
  content: "";
  position: absolute;
  top: -12px;
  top: -6px;
  border-right: 3px solid #fff;
  border-top: 3px solid #fff;
  border-left: 3px solid transparent;
  border-bottom: 3px solid transparent;
  width: 6px;
  height: 6px;
  background: inherit;
}
.widget.soft-senser-monitoring .tooltip-box::after {
  content: "";
  position: absolute;
  width: 15px;
  height: 39px;
  background: linear-gradient(90deg, rgba(1,1,1,0.3) 0%, rgba(1,1,1,0.2) 2%, rgba(1,1,1,0.2) 4%, rgba(1,1,1,0.0) 100%);
  top: 0;
  left: 0;
  opacity: 0.8;
}
.widget.soft-senser-monitoring .tooltip-box-inner {
  padding: 5px 7px;
}
.widget.soft-senser-monitoring .tooltip-box-inner .date{
  font-size: 0.75rem;
  color: #666;
}
.widget.soft-senser-monitoring .tooltip-box-inner .text{
  white-space: nowrap;
  font-size: 0.85rem;
  color: #111;
}

.stack .line .tooltip-box  {
  display: none;
  z-index: 10
}
.stack .line:hover .tooltip-box {
  display: block;
}
.bop .line .tooltip-box  {
  display: block;
}

