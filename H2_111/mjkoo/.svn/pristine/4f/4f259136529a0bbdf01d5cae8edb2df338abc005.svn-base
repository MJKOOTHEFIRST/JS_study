.widget.search-result .scrollmini.result tr {
  position: relative;
}
.widget.search-result .scrollmini.result tr td:last-child {
  overflow: visible;
  padding: 0;
  background: #f2f2f2;
  background-image: url("data:image/svg+xml,%3Csvg width='7px' height='7px' viewBox='0 0 9 9' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='M0 0H9V9L0 0Z' fill='%23BBBBBB'/%3E%3C/svg%3E");
  background-repeat: no-repeat no-repeat;
  background-position: top right;
  /* background-size: cover; */
}
.widget.search-result .scrollmini.result tr td:last-child input[type="text"] { 
  background: transparent;
  border: none;
  width: 95%;
  margin: 4px 0;
  text-align: center; 
}

.widget.search-result .scrollmini.result tr td:last-child input[type="text"]:focus { 
  position: absolute;
  border: 1px solid #767676;
  padding-left: 5px;
  width: calc(84% - 30px);
  background: #fff;
  right: 3px;
  text-align: left;
  box-shadow: 0px 0px 10px rgba(0,0,0,0.7);
}

