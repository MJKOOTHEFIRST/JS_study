  background: #ECF6FF;
}
.widget.HW-BOP-sensor-info .sensor.heat {
  background: #FFF3F3;
}
.widget.HW-BOP-sensor-info .sensor.generator {
  background: #FEFFF1;
}
.widget.HW-BOP-sensor-info .sensor.warning {
  border: 1px solid #F00;
  border-left: 5px solid #F00;
  color: #D00;
  animation-name: senserAnimation;
  animation-duration: 0.3s;
  animation-iteration-count: infinite;
  will-change: opacity;
}
@keyframes senserAnimation {
  0% {opacity: 0;}
  100% {opacity: 1;}
}


